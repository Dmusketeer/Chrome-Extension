{
    "manifest_version":2,
    "name":"Budget Manager",
    "version":"1.0.0",
    "description":"This exetention will calculate your overall spending.",
    "icons":{
        "128":"icon128.png",
        "48":"icon48.png",
        "16":"icon16.png"
    },
    "browser_action":{
        "default_icon":"icon128.png",
        "default_popup":"popup.html"
    },
    "permissions" : [
        "storage","activeTab"
    ],
    "background":{
        "Scripts":["./popup.js"]
    }
    

}