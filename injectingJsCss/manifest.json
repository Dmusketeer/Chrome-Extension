{
    "manifest_version":2,
    "name":"Injecting Css Js",
    "version":"1.0.0",
    "description":"How js run in background and foreground",
    "icons":{
        "16":"./icon_16.png",
        "32":"./icon_32.png",
        "48":"./icon_48.png",
        "128":"./icon_128.png"
    },
    "background":{
        "scripts":["./background.js"]
    },
    "browser_action":{
        "default_popup":"index.html",
        "default_icon":"./logo.png"
    },
    "permissions":[
        "tabs","activeTab","https://www.google.com/*"
    ]
}